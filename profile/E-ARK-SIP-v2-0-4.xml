                        <dt>Cardinality</dt><dd>1..1</dd>
                    </dl>
                </description>
            </requirement>
            <requirement ID="SIP28" REQLEVEL="MUST" EXAMPLES="metsHdrElementExample1">
                <description>
                    <head>Preservation agent type</head>
