                    <dl xmlns="http://www.w3.org/1999/xhtml">
                        <dt>METS XPath</dt><dd>metsHdr/agent</dd>
                        <dt>Cardinality</dt><dd>0..1</dd>
                    </dl>
                </description>
            </requirement>
            <requirement ID="SIP27" REQLEVEL="MUST" EXAMPLES="metsHdrElementExample1">
