                    <dl xmlns="http://www.w3.org/1999/xhtml">
                        <dt>METS XPath</dt><dd>metsHdr/agent/note</dd>
                        <dt>Cardinality</dt><dd>0..1</dd>
                    </dl>
                </description>
            </requirement>
            <requirement ID="SIP31" REQLEVEL="MUST" RELATEDMAT="VocabularyNoteType" EXAMPLES="metsHdrElementExample1">
